/* pages/lovelist/lovelist.wxss */
.filter{
  background-image: linear-gradient(#febac9, #ffc39f);
  border-radius: 6rpx;
  display: inline-block;
  padding: 10rpx;
  margin-right: 25rpx;
  margin-top: 10rpx;
  
}
.listColumn{
  padding: 0 25rpx;
}
.title{
  font-size: 30rpx;
  margin-left: 26rpx;
  font-weight: 700;
}
.content{
  padding: 0 25rpx;
  display: flex;
  flex-wrap: wrap;
}
.activecontentItem{
  background:#ffe2e4 url(data:image/png;base64,) no-repeat !important;
  background-size:100% 100% !important;
  color:#fd756e;
}
.agecontentItem{
  border-radius: 6rpx;
  text-align: center;
  padding: 18rpx 22rpx;
  background: #f9f9fb;
  margin: 0 10rpx 16rpx;
}
.contentItem{
  width: 110rpx;
  border-radius: 6rpx;
  text-align: center;
  padding: 8rpx 22rpx;
  background: #f9f9fb;
  margin-right: 10rpx;
}

.btns{
  margin-top: 30rpx;
  padding-bottom: 20rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
}