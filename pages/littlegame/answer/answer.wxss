/* pages/me/me.wxss */
.top{
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 0 25rpx;
    height: 432rpx;
    background: url("data:image/png;base64,");
  }

  .appWrap{
    width: 100%;
    position: relative;
  }
  .app{
    margin: -110rpx 25rpx 0;
    padding: 78rpx 48rpx;
    border-radius: 15px 15px 0px 0px;
    
    background: #fff;
  }
  .orderNum{
      text-align: center;
      line-height: 80rpx;
      font-size: 30rpx;
      color: #fff;
    width: 244rpx;
    height:  80rpx;
    position: absolute;
    left: 50%;
    top: -24rpx;
    margin-left: -122rpx;
    background: url("data:image/png;base64,") no-repeat;
    background-size: 244rpx 80rpx;
  }
  .question{
      font-size: 36rpx;
      text-align: center;
      font-weight: 700;
      margin: 30rpx 0 60rpx;
  }
  .answer{
      text-align: center;
      display: flex;
      flex-direction: column;
      align-items: center;
  }
  .answerColumn{
      width: 500rpx;
      height: 80rpx;
      background: #f9f9f9;
      border-radius: 10rpx;
      display: flex;
      align-items: center;
      margin-bottom: 36rpx;
  }
  .active .order{
    background: #ff8e92;
  }
  .active{
      background: #ffe8e9;
  }
  .order{
    width: 50rpx;
    height: 50rpx;
    line-height: 50rpx;
    background: #333;
    border-radius: 50%;
    color: #fff;
    margin: 0 38rpx 0 16rpx;
}
.answerCotent{
    font-size: 36rpx;
	font-weight: 700;
}
.rightKey{
    color: #fe2d5e;
    width: 500rpx;
    padding: 26rpx 36rpx;
    background: #ffe8e9;
    border-radius: 10rpx;
}
.bts{
    display: flex;
    justify-content: space-around;
    margin-top: 36rpx;
    width: 500rpx;
}
.btn_class{width: 200rpx;}

  
  